{
  "name": "ex1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./built/run.js",
    "dev": "DEBUG=* nodemon",
    "build": "./node_modules/.bin/ts-node build.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, jade",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src/run.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "fs-extra": "^10.1.0",
    "helmet": "^5.0.2",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0"
  }
}
